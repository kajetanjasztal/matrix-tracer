{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "lib": ["es2015", "dom"],
    "declaration": true,
    "outDir": "js",
    "removeComments": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
